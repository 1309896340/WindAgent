{
  // 使用 IntelliSense 了解相关属性。
  // 悬停以查看现有属性的描述。
  // 欲了解更多信息，请访问: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python 调试程序: 当前文件",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "启动ai后端",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "args": ["AiAgent.main:app", "--reload"],
      "jinja": true
    },
    {
      "name": "启动ai console端",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/demo/main.py",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    }
  ]
}
